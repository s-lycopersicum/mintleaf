        "@types/react": "^19.1.5",
        "@types/react-dom": "^19.1.5",
        "eslint": "^9.27.0",
        "eslint-plugin-react": "^7.37.5",
        "parcel": "^2.15.1",
        "rimraf": "^6.0.1",
        "typescript-eslint": "^8.32.1"
    },
    "dependencies": {
        "react": "^19.1.0",
        "react-dom": "^19.1.0"
    }
