{
    "name": "mintleaf",
    "version": "0.0.0",
    "description": "A multipurpose node-based tool for the browser.",
    "license": "MIT",
    "author": "Lico (https://github.com/s-lycopersicum)",
    "private": true,
    "type": "module",
    "source": "src/index.html",
